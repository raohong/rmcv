{
  "private": true,
  "name": "rmc-vant--root",
  "scripts": {
    "css-var": "ts-node ./scripts/cssVars.ts",
    "build": "ts-node ./scripts/build.ts",
    "precommit": "lint-staged",
    "docs:watch": "ts-node ./docs/scripts/buildDocs.ts",
    "lint": "pnpm lint:style & pnpm lint:script",
    "lint:style": "stylelint 'packages/*/src/**/*.less'",
    "lint:script": "eslint 'packages/*/src/**/*.{ts,tsx}'",
    "lint-fix:script": "pnpm lint:script --fix",
    "lint-fix:style": "pnpm lint:style --fix",
    "lint-fix": "pnpm lint-fix:script && pnpm lint-fix:style",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,mdx,json,cjs}'",
    "tsc": "tsc --noEmit",
    "test-ct:run": "cypress run-ct",
    "test-ct:open": "cypress open-ct",
    "test-jest": "jest",
    "test": "concurrently \"pnpm test-ct:run\" \"pnpm test-jest\"",
    "test:update": "jest -u",
    "version": "pnpm changeset version",
    "publish": "pnpm changeset publish"
  },
  "repository": {
    "url": "https://github.com/raohong/rmcv.git"
  },
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "lint-staged": {
    "*.{js,jsx,less,md,json,cjs}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "resolutions": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.24.3",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@cypress/react": "^5.10.1",
    "@cypress/webpack-dev-server": "^1.6.0",
    "@octokit/auth-token": "^2.4.5",
    "@octokit/rest": "^18.9.0",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^29.1.2",
    "@types/node": "18.7.14",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "chalk": "^4.1.2",
    "concurrently": "^6.3.0",
    "cypress": "^10.10.0",
    "del": "^6.1.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^43.0.0",
    "gh-pages": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-tsb": "^4.0.6",
    "husky": "^8.0.1",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "lint-staged": "^10.0.7",
    "p-queue": "~6.6.0",
    "prettier": "^2.7.1",
    "prettier-plugin-css-order": "^1.3.0",
    "react-scripts": "^4.0.3",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.5.0",
    "webpack-dev-server": "^4.10.0"
  },
  "devDependencies": {
    "eslint-plugin-babel": "^5.3.1",
    "postcss-less": "^6.0.0"
  }
}
