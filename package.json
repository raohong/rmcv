{
  "private": true,
  "name": "rmc-vant--root",
  "scripts": {
    "start": "dumi dev",
    "bootstrap": "lerna bootstrap",
    "compile": "node ./scripts/compile",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "precommit": "lint-staged",
    "lint": "yarn lint:style & yarn lint:script",
    "lint:style": "stylelint 'packages/*/src/**/*.less' --syntax less",
    "lint:script": "eslint 'packages/*/src/**/*.{ts,tsx}'",
    "lint-fix:script": "npm run lint:script -- --fix",
    "lint-fix:style": "npm run lint:style -- --fix",
    "lint-fix": "npm run lint-fix:script && npm run lint-fix:style",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "tsc": "tsc --noEmit",
    "test": "jest",
    "test:update": "jest -u",
    "postinstall": "yarn bootstrap",
    "release": "lerna version",
    "publish": "lerna publish from-git"
  },
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "resolutions": {
    "@types/react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@octokit/auth-token": "^2.4.5",
    "@octokit/rest": "^18.9.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "chalk": "^4.1.2",
    "dumi": "1.1.25",
    "dumi-theme-mobile": "1.1.8",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-unicorn": "^35.0.0",
    "gh-pages": "^3.0.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lerna": "^4.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.0.4",
    "typescript": "4.3.5",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@types/react": "^16.9.0"
  }
}
