{
  "name": "rmc-vant--root",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "url": "https://github.com/raohong/rmcv.git"
  },
  "scripts": {
    "build": "pnpm --filter \"./packages/**/*\" run build",
    "prepublishOnly": "father doctor && npm run build",
    "precommit": "lint-staged",
    "docs:deploy": "pnpm docs:build && pnpm --filter \"./docs\" run build && pnpm --filter \"./docs\" run start -p 3700",
    "docs:watch": "cross-env WATCH=true ts-node ./docs/scripts/buildDocs.ts",
    "docs:dev": "pnpm --filter \"./docs\" run dev -p 3700",
    "docs:build": "ts-node ./docs/scripts/buildDocs.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsc": "tsc --noEmit",
    "test-jest:headless": "corss-env HEADLESS=false jest",
    "test-jest:coverage": "pnpm test --coverage",
    "test": "jest",
    "test-jest:update": "pnpm test-jest -u",
    "version": "pnpm changeset version",
    "publish": "pnpm changeset publish --access public"
  },
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.24.3",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@emotion/react": "~11.10.5",
    "@emotion/styled": "~11.10.5",
    "@octokit/auth-token": "^2.4.5",
    "@octokit/rest": "^18.9.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.7",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.0",
    "@types/node": "18.7.14",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "cross-env": "^7.0.3",
    "del": "^6.1.1",
    "eslint-plugin-format": "^0.1.2",
    "father": "~4.3.1",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-puppeteer": "^10.0.1",
    "lint-staged": "^10.0.7",
    "p-queue": "~6.6.0",
    "prettier": "^3.3.0",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "ts-jest": "29.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "~2.23.1",
    "@emotion/cache": "^11.10.5",
    "@emotion/jest": "^11.10.5",
    "@eslint-react/eslint-plugin": "^1.7.1",
    "eslint": "8.57.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "postcss-less": "^6.0.0"
  },
  "resolutions": {
    "rc-field-form": "1.26.4"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
